# DEFAULT SESSION - Basic tabs for common locations

new_tab 🏠 HOME
cd ~
launch bash

new_tab 💼 PROJECTS
launch bash -lc 'cd ~/projects 2>/dev/null || { mkdir -p ~/projects && cd ~/projects; }; exec ${SHELL:-bash}'

new_tab ⚙️ CONFIG
cd ~/.config
launch bash

new_tab 📥 DOWNLOADS
launch bash -lc 'cd ~/Downloads 2>/dev/null || { mkdir -p ~/Downloads && cd ~/Downloads; }; exec ${SHELL:-bash}'

goto_tab 1
